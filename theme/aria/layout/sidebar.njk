{% block sidebar %}
<aside class="sidebar" id="sidebar" >
  {% if theme.search %}
  <div class="search">
    <div class="form-group">
      <i class="fas fa-search"></i><input type="search" id="search-input" name="q" results="0" placeholder="{{ __("search") }}" class="form-control"/>
    </div>
  </div>
  <div class="search-result-box" id="search-result"></div>
  {% endif %}
  {% include "info.njk" %}
  <div >
    {% if page.content %}
    {% include "toc.njk" %}
    {% endif %}
    {% if theme.social.enable %}
    {% include "sociallink.njk" %}
    {% endif %}
    {% if theme.blogroll.enable %}
    {% include "blogroll.njk" %}
    {% endif %}
  </div>
  <div class="sidebar-sticky">
      <iframe frameborder="no" border="0" marginwidth="0" marginheight="0" width=330 height=450 src="//music.163.com/outchain/player?type=0&id=2317836514&auto=1&height=430"></iframe>
  </div>
</aside>
{% endblock %}
